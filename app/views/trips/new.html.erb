TripsController#New


<%= simple_form_for @trip do |f| %>
  <%= f.input :city  %>
  <%= f.input :start_date %>
  <%= f.input :end_date %>
  <%= f.input :start_city %>
  <%= f.input :end_city %>
  <%= f.input :price %>
  <%= f.input :jet_name %>
  <%= f.input :jet_model %>
  <%= f.input :jet_seats %>
  <%= f.input :jet_description %>

  <%= f.submit "add a trip", class: "btn btn-primary" %>
<% end %>
